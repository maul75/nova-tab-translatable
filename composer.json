{
    "name": "kongulov/nova-tab-translatable",
    "description": "Making Nova Tab Translatable",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/kongulov/nova-tab-translatable",
    "license": "MIT",
    "authors": [
        {
            "name": "Ramiz Kongulov",
            "email": "ramiz.kongulov@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-translatable": "^5.0|^6.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Kongulov\\NovaTabTranslatable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kongulov\\NovaTabTranslatable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
